@model IEnumerable<SeanceWebAppModel.Seance>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IDSport)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Horaire)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Duree)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IDSport)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Horaire)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Duree)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IDGymnase">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IDGymnase">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IDGymnase">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
